    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.21.1",
    "chart.js": "^3.5.1",
    "connected-react-router": "^6.1.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^7.0.3",
    "flatted": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.1",
    "redux-axios-middleware": "4.0.1",
